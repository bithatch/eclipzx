#include "lib/tst_framework.bas"

INIT("Testing ON .. GOTO, ON .. GOSUB")


DIM a as UBYte = 1
ON a + 1 GOTO 10, 20, 30

10 PRINT "10"
20 PRINT "20"
30 PRINT "30"

ON a + 2 GOSUB 40, 50, 60, 70
ON 1 GOSUB 50, 60
REPORT_OK

END
40 PRINT "40": RETURN
50 PRINT "50": RETURN
60 PRINT "60": RETURN
70 PRINT "70": RETURN

